<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/today_purple_1">

        <ImageView
            android:id="@+id/iv_main_top"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:src="@drawable/shape_white_fill_0_rect"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iv_main_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/iv_logo"
            app:layout_constraintBottom_toBottomOf="@id/iv_main_top"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:src="@drawable/ic_back"
            app:layout_constraintStart_toStartOf="@id/iv_main_top"
            app:layout_constraintTop_toTopOf="@id/iv_main_top" />

        <ImageView
            android:id="@+id/iv_diary_box_1"
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/shape_white_fill_30_rect"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv_main_logo" />

        <ImageView
            android:id="@+id/star1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="110dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/iv_star"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_1"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_1" />

        <ImageView
            android:id="@+id/star2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="137dp"
            android:src="@drawable/iv_star"
            app:layout_constraintStart_toStartOf="@+id/star1"
            app:layout_constraintTop_toTopOf="@+id/star1" />

        <ImageView
            android:id="@+id/star3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:src="@drawable/iv_star"
            app:layout_constraintStart_toStartOf="@+id/star1"
            app:layout_constraintTop_toTopOf="@+id/star1" />

        <TextView
            android:id="@+id/diary_location_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="잠실(jamsil)"
            android:textColor="@color/black"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_1"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_1"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_1"
            app:layout_constraintVertical_bias="0.41" />

        <TextView
            android:id="@+id/diary_content_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="잠실 석촌호수 산책길"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_1"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_1"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_1"
            app:layout_constraintVertical_bias="0.6" />

        <TextView
            android:id="@+id/diary_content_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="유미카츠 잠실점"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_1"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_1"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_1"
            app:layout_constraintVertical_bias="0.745" />

        <TextView
            android:id="@+id/diary_content_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="셜록홈즈 방탈출 잠실점"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_1"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_1"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_1"
            app:layout_constraintVertical_bias="0.899" />

        <ImageView
            android:id="@+id/iv_diary_box_2"
            android:layout_width="match_parent"
            android:layout_height="500dp"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/shape_white_fill_30_rect"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_diary_box_1" />

        <TextView
            android:id="@+id/diary_content_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="128dp"
            android:text="2023/5/13/sat"
            android:textColor="@color/black"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_2"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_2"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_2"
            app:layout_constraintVertical_bias="0.052" />

        <TextView
            android:id="@+id/diary_content_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:text="<오늘은 팀원들이랑 맛집탐방~>"
            android:textColor="@color/black"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_2"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_2"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_2"
            app:layout_constraintVertical_bias="0.13" />

        <TextView
            android:id="@+id/diary_content_6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="15dp"
            android:text="@string/detail_content"
            android:textColor="@color/black"
            android:textSize="15dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_diary_box_2"
            app:layout_constraintEnd_toEndOf="@+id/iv_diary_box_2"
            app:layout_constraintStart_toStartOf="@id/iv_diary_box_2"
            app:layout_constraintTop_toTopOf="@id/iv_diary_box_2"
            app:layout_constraintVertical_bias="0.35" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>